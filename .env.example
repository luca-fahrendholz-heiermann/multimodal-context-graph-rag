# Optional: install docling dependencies on container startup (0/1)
RAG_INSTALL_DOCLING=1

# Optional: prefill email recipient in UI
RAG_DEFAULT_EMAIL_RECIPIENT=rag-inbox@example.local

# Optional: custom CORS list for backend
RAG_CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Optional: LLM image analysis provider config
RAG_IMAGE_DESCRIPTION_PROVIDER=
RAG_IMAGE_DESCRIPTION_OPENAI_MODEL=gpt-4.1-mini
RAG_IMAGE_DESCRIPTION_GEMINI_MODEL=gemini-1.5-flash

# Optional: bring your own provider keys (leave empty in demo mode)
OPENAI_API_KEY=
GEMINI_API_KEY=
